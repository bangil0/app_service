<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.micro.msb.dao.mapper.DaoMapper">			
		
	<resultMap id="appGroupAuthoritiesReadResult" type="AppGroupAuthoritiesReadBean">
		<result property="groupId" column="group_id"/>
		<result property="authority" column="authority"/>
	</resultMap>
	
	<select id="readAppGroupAuthorities" parameterType="com.micro.msb.beans.AppGroupAuthoritiesReadBean" resultMap="appGroupAuthoritiesReadResult">         
		select * from app_group_authorities where group_id = #{groupId};
	</select>

</mapper>
